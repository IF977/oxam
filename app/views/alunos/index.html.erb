<p id="notice"><%= notice %></p>

<p><a class="btn btn-primary" href="/alunos/new">Novo Aluno</a></p><br>

<div class="row">
    <div class="col-lg-12">
        <div class="ibox float-e-margins">
            <div class="ibox-title">
                <h5>Alunos</h5>

            </div>
            <div class="ibox-content">

                <table class="table">
                    <thead>
                        <tr>
                          
      <th>Instituição</th>
      <th>Nome</th>
      <th>Data nasc</th>
      <th>Cidade</th>
      <th>Email</th>
      <th>Login</th>
      <th colspan="3"></th>
                        </tr>
                    </thead>
                    <tbody>

   <% @alunos.each do |aluno| %>
      <tr>
        <td><%= aluno.p_nome_inst %></td>
        <td><%= aluno.p_nome %></td>
        <td><%= aluno.p_data_nasc %></td>
        <td><%= aluno.p_cidade %></td>
        <td><%= aluno.p_email %></td>
        <td><%= aluno.p_login %></td>
        <td><%= link_to 'Exibir', aluno %></td>
        <td><%= link_to 'Editar', edit_aluno_path(aluno) %></td>
        <td><%= link_to 'Excluir', aluno, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>


                    </tbody>
                </table>

            </div>
        </div>
    </div>

</div>


