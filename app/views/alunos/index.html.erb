<p id="notice"><%= notice %></p>

<h1>Listing Alunos</h1>

<table>
  <thead>
    <tr>
      <th>P nome</th>
      <th>P data nasc</th>
      <th>P endereco</th>
      <th>P bairro</th>
      <th>P numero</th>
      <th>P cidade</th>
      <th>P complemento</th>
      <th>P cep</th>
      <th>P email</th>
      <th>P nome inst</th>
      <th>P login</th>
      <th>P senha</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @alunos.each do |aluno| %>
      <tr>
        <td><%= aluno.p_nome %></td>
        <td><%= aluno.p_data_nasc %></td>
        <td><%= aluno.p_endereco %></td>
        <td><%= aluno.p_bairro %></td>
        <td><%= aluno.p_numero %></td>
        <td><%= aluno.p_cidade %></td>
        <td><%= aluno.p_complemento %></td>
        <td><%= aluno.p_cep %></td>
        <td><%= aluno.p_email %></td>
        <td><%= aluno.p_nome_inst %></td>
        <td><%= aluno.p_login %></td>
        <td><%= aluno.p_senha %></td>
        <td><%= link_to 'Show', aluno %></td>
        <td><%= link_to 'Edit', edit_aluno_path(aluno) %></td>
        <td><%= link_to 'Destroy', aluno, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Aluno', new_aluno_path %>
